{{ form_start(form) }}
<div>
    <div class="form-group p-3">
        {{ form_row(form.name,{'attr':{
            'class':'form-control'
        }}) }}
    </div>
    <div class="form-group p-3">
        {{ form_row(form.prenom, {'attr':{
            'class': 'form-control'
        }}) }}
    </div>
    <div class="form-group p-3">
        {{ form_row(form.adresse, {'attr':{
            'class': 'form-control'
        }}) }}
    </div>
    <div class="form-group p-3">
        {{ form_row(form.email, {'attr':{
            'class': 'form-control'
        }}) }}
    </div>
    <div class="form-group p-3">
        {{ form_row(form.phone, {'attr':{
            'class': 'form-control'
        }}) }}
    </div>

    <div class="form-group p-3">
        {{ form_row(form.CIN, {'attr':{
            'class': 'form-control'
        }}) }}
    </div>

</div>
<div style="text-align: center; padding-bottom: 10px">
    <button class="btn btn-dark text-white p-1 "style="text-decoration: none;">{{ button_label|default('Save') }}</button>
</div>
{{ form_end(form) }}
