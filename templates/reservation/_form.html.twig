{{ form_start(form) }}
<div>
    <div class="form-group p-3">
        {{ form_row(form.dateDepart,{'attr':{
            'class':'form-control'
        }}) }}
    </div>
    <div class="form-group p-3">
        {{ form_row(form.dateRetoure, {'attr':{
            'class': 'form-control'
        }}) }}
    </div>
    <div class="form-group p-3">
        {{ form_row(form.nbrPersonne, {'attr':{
            'class': 'form-control'
        }}) }}
    </div>
    <div class="form-group p-3">
        {{ form_row(form.user, {'attr':{
            'class': 'form-control'
        }}) }}
    </div>
    <div class="form-group p-3">
        {{ form_row(form.client, {'attr':{
            'class': 'form-control'
        }}) }}
    </div>
    <div class="form-group p-3">
        {{ form_row(form.voyage, {'attr':{
            'class': 'form-control'
        }}) }}
    </div>
</div>
<div style="text-align: center; padding-bottom: 10px">
    <button class="btn btn-dark text-white p-1 "style="text-decoration: none;">{{ button_label|default('Save') }}</button>
</div>
{{ form_end(form) }}
